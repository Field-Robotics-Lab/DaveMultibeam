@Article{Walker,
  author       = {Shane C. Walker and Caglar Yardim and Aaron Thode and Ery Arias-Castro},
  title        = {Using Fisher information to quantify uncertainty in environmental parameters estimated from correlated ambient noise},
  number       = {4},
  pages        = {EL228-EL234},
  volume       = {133},
  comment      = {Uses a multivariate Gaussian approximation to the likelihood function, where the covariance matrix, \Sigma, is the channel covariance matrix of the array. The Gaussian assumption allows the partial derivatives in the Fisher information matrix to performed wrt the covariance matrix, rather than to the likelihood. This is where Siderius and Gebbie 2019 obtain their formula. I believe it's derived from Kay's statistical signal processing in the present paper.

Provides formulas for both N independent measurements, as well as continuous measurements.

Simple analytically tractible case for two sensors measureing sound speed and atteenuation. At small separation, there is a large SNR, but small aperture. At large separations, there is a large aperture, but smaller SNR. There is a sweet spot of about 10 lambda that maximizes the Fisher information.},
  creationdate = {2020-10-01T00:00:00},
  date         = {2013},
  doi          = {10.1121/1.4792836},
  file         = {:/Users/derek/Library/Mobile Documents/com~apple~CloudDocs/Literature/currentReading/1.4792836.pdf:PDF},
  groups       = {Read and Annotated},
}

