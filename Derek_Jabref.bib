@Article{Wagner1967,
  author       = {R. J. Wagner},
  journal      = JASA,
  title        = {Shadowing of Randomly Rough Surfaces},
  year         = {1967},
  pages        = {138 - 147},
  volume       = {41},
  comments     = {Statistical shelf-shadowing of a rough surface with gaussian statistics and gaussian correlation are examined. The us of this shadowing function is slightly suspect. It is based on heuristic arguments with single-scattering assumed. A summary of results are presented in Jackson's composite roughness paper. Interesting things to note: diffraction is not included. Shaodowin not only reduces the illuminated area by also skews the slope and height distributions positively and the surface will act as if tilted away. This result is ONLY applicable to the high-frequency kirchhoff approximation. The perturbation approximation does not use the same formalism and is a resonant effect. So long as the horizontal wavenumber is roughly constant over the incident field, the patchwork illumination caused by statistical self-shadowing probably does not affect the physical scattering mechanisms because of the resonant effect. The total incident energy will be the same on the mean field, but just distributed differently across the rough field. I don't think shadowing is important for the perturbation theory result, but may improve the accuracy of the Kirchhoff approximation.},
  creationdate = {2020-10-01T00:00:00},
  issue        = {1},
  pdf          = {y},
  printed      = {y},
}

