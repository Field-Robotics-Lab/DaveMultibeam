@PhdThesis{Brown2017b,
  author       = {Daniel C. Brown},
  title        = {Modeling and Measurement of Spatial Coherence for Normal Incidence Seafloor Scattering},
  comment      = {<The spatial coherence analysis is a bit like estimating the spectrum of a windowed random process. Dan's assumptions state that the random process has a flat, bandlimited,  spectrum and a delta function spatial autocovariance. More general models should include the autocovariance of the scattered field at the interface. Perturbation theory dictates that the field at a the surface is equal to
\[
p_s(x,y) = - f(x,y) \frac{\partial p_0}{\partial z}
\]

Therefore the auto covariance of the field is related to the autocovariance of the random rough surface. If the surface has a huge correlation length, then p_s will be correlated over large spatial separations. HOweer, the rms roughnesss is usually bigger as you increase the ensonified area, so first-order perturbation theory is unlikely to be a good approximation. Second order perturbation theory is
\[
p^{(1)}_s(x,y) = f^2(x,y) \frac{\partial p^1(x,y)}{\partial z}
\]

To form a consistent covariance, we need to use the second moment,
\[
\left \langle p p\ast \right \rangle = %
	\left \langle p_{1} p_{1}^\ast + p_{2} p_2^\ast + p_1 p_3^\ast + p_1^\ast p_3 \right \rangle
\]

My guess is that the second-order terms (inensity to order (kh)^4) will decrease the correlation length. The 13 terms often cancel out much of the 22 terms.},
  creationdate = {2020-09-01T00:00:00},
  date         = {01-05-2017},
  file         = {:Brown2017b - Modeling and Measurement of Spatial Coherence for Normal Incidence Seafloor Scattering.pdf:PDF},
  institution  = {The Pennsylvania State University},
}

